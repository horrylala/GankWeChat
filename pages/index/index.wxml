<!--index.wxml-->
<view class="page">
  <view class="index">
    <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:for="{{imageUrls}}">
        <swiper-item>
          <image src="{{item.url}}" style=" width: 100%; height: 100%; top: 0px; left: 200%;"></image>
        </swiper-item>
      </block>
    </swiper>
    <block wx:for="{{pageData}}" class="container">
      <view class="section_title">{{index}}</view> 
      <view wx:for="{{item}}" class="section_content">
      <view>{{item.desc}}</view>
      <view wx:for="{{item.images}}">   
      <image style="display:{{item == ''}} ? none ,flex;" src="{{item}}"></image>
      </view>
      </view> 
      <!--<image  src="http://wx.qlogo.cn/mmhead/DYAIOgq83eqNnDuMoPQ3lYb07MSvzMXOic6pUf74ocpnYwdhYvZNbKQ/132" background-size="cover"></image>-->
    </block>
</view>
<view class="float-action" bindtap="ballClickEvent" touchmove="ballMoveEvent" style="opacity:20%;bottom:20px;right:20px;"></view>
<view class="slide-mask" style="display:{{maskDisplay}}" bindtap="slideCloseEvent"></view>
<view class="slide-menu" animation="{{slideAnimation}}">
  <navigator url="../category/category?category=all" hover-class="navigator-hover">all</navigator>
  <navigator url="../category/category?category=Android" hover-class="navigator-hover">Android</navigator>
  <navigator url="../category/category?category=休息视频" hover-class="navigator-hover">休息视频</navigator>
  <navigator url="../category/category?category=福利" hover-class="navigator-hover">福利</navigator>
  <navigator url="../category/category?category=拓展资源" hover-class="navigator-hover">拓展资源</navigator>
  <navigator url="../category/category?category=前端" hover-class="navigator-hover">前端</navigator>
  <navigator url="../category/category?category=瞎推荐" hover-class="navigator-hover">瞎推荐</navigator>
  <navigator url="../category/category?category=App" hover-class="navigator-hover">App</navigator>
</view>
</view>



