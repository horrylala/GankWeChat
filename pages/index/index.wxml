<!--index.wxml-->
<view class="page">
  <view class="index">
    <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:for="{{imageUrls}}">
        <swiper-item>
          <image src="{{item}}" class="slide-image"></image>
        </swiper-item>
      </block>
    </swiper>
    <block wx:for="{{pageData}}" class="container">
      <view class="section_title">{{item}}</view> 
      <view class="section_content">
      <view></view>
      </view> 
      <!--<image  src="http://wx.qlogo.cn/mmhead/DYAIOgq83eqNnDuMoPQ3lYb07MSvzMXOic6pUf74ocpnYwdhYvZNbKQ/132" background-size="cover"></image>-->
    </block>
</view>
</view>

